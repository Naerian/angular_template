{
  "name": "neo-web-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:dev": "ng build --configuration dev",
    "build:pre": "ng build --configuration pre",
    "build:pro": "ng build --configuration pro ",
    "watch": "ng build --watch --configuration dev",
    "test": "ng test",
    "compodoc": "npx compodoc --language es-ES --theme readthedocs --disableDomTree --hideGenerator --minimal --silent --name \"Documentación técnica\" --customFavicon \"./src/favicon.ico\" --customLogo \"./src/assets/images/logo/logo_full_250.png\" -p tsconfig.doc.json",
    "sonar": "sonar-scanner",
    "storybook": "ng run uem-sis-app:storybook",
    "build-storybook": "ng run uem-sis-app:build-storybook"
  },
  "private": true,
  "dependencies": {
